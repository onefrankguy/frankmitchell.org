<ul>
<% @posts.each do |year, words| %>
<li class="sect"><time datetime="<%= year %>"><%= year %></time></li>
<% words.each do |post| %>
<li>
<a title="Read &ldquo;<%= post['title'] %>&rdquo;" href="<%= post['url'] %>"><%= post['title'] %></a>
<time datetime="<%= post['date']['time'] %>" title="<%= post['date']['title'] %>"><%= post['date']['abbr'] %></time></td>
</li>
<% end %>
<% end %>
</ul>
